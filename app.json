{
  "expo": {
    "name": "murmur.",
    "slug": "murmur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#F2EFE9"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yourname.murmur"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F2EFE9"
      },
      "package": "com.yourname.murmur"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#5E7A8A",
          "sounds": []
        }
      ]
    ],
    "newArchEnabled": true
  }
}